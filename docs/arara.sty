% bash
% -------------------------------------------------
\lstnewenvironment{bash}{\lstset{%
   language=bash,
   basicstyle=\ttfamily,
   columns=flexible,
   showspaces=false,
   xleftmargin=1.5em,
   breaklines=true,
   numberstyle=\ttfamily\small,
   %breakatwhitespace=true,
   showstringspaces=false
}}{}

\lstnewenvironment{nolanguage}{\lstset{%
   basicstyle=\ttfamily,
   columns=flexible,
   showspaces=false,
   xleftmargin=1.5em,
   breaklines=true,
   numberstyle=\ttfamily\small,
   breakatwhitespace=true,
   showstringspaces=false
}}{}
