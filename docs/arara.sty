% Java
% -------------------------------------------------
\lstnewenvironment{java}{\lstset{%
   language=Java,
   basicstyle=\ttfamily,
   columns=flexible,
   showspaces=false,
   numbers=left,
   xleftmargin=1.5em,
   breaklines=true,
   numberstyle=\ttfamily\small,
   breakatwhitespace=true,
   showstringspaces=false
}}{}
% -------------------------------------------------

% C
% -------------------------------------------------
\lstnewenvironment{clang}{\lstset{%
   language=C,
   basicstyle=\ttfamily,
   columns=flexible,
   showspaces=false,
   numbers=left,
   xleftmargin=1.5em,
   breaklines=true,
   numberstyle=\ttfamily\small,
   breakatwhitespace=true,
   showstringspaces=false
}}{}
% -------------------------------------------------

% LaTeX
% -------------------------------------------------
\lstnewenvironment{latex}{\lstset{%
   language=[LaTeX]TeX,
   basicstyle=\ttfamily,
   columns=flexible,
   showspaces=false,
   numbers=left,
   xleftmargin=1.5em,
