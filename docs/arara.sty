% language environments

% YAML
% -------------------------------------------------
\lstnewenvironment{yaml}{\lstset{%
   basicstyle=\ttfamily,
   numbers=left,
   xleftmargin=1.5em,
   breaklines=true,
   numberstyle=\ttfamily\small,
   columns=flexible,
   mathescape=false
}}{}
